class Solution {
    // function to print alternate elements of an array
    public ArrayList<Integer> getAlternates(int arr[]) {
        // Code Here
        int n=arr.length;
        ArrayList<Integer> l=new ArrayList<>(n/2+1);
        for(int i=0;i<n;i+=2)
        {
            l.add(arr[i]);
        }
        return l;
    }
}
