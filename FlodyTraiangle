package javapac;
import java.util.*;
public class FlodyTraiangle {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt(),sum=1;
		for(int i=1;i<=n;i++)
		{
			for(int j=1;j<=i;j++) {
				System.out.print(sum+" ");
				sum+=1;
			}System.out.println();
		}
	}

}
