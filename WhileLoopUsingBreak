package javapac;
import java.util.*;
public class WhileLoopUsingBreak {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int n;//=sc.nextInt();
		while(true) {
			n=sc.nextInt();
			System.out.println("you enter : "+n);
			if(n<0) {
				break;
			}
		}

	}

}
