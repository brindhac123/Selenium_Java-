class Solution {
    // Function to reverse words in a given string
    public String reverseWords(String s) {
        // Code here
        String arr[]=s.trim().split("\\s+"),reverse="";
        StringBuilder sb=new StringBuilder();
        for(int i=arr.length-1;i>=0;i--){
            sb.append(arr[i]);
            if(i!=0) sb.append(" ");
        }
        return sb.toString();
    }
}
