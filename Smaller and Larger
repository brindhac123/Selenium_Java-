class Solution {
    int[] getMoreAndLess(int[] arr, int target) {
        // code here
        int c=0,d=0;
        int ar[] = new int[2];
        for(int i=0;i<arr.length;i++){
            if(arr[i]<=target)
            c++;
            if(arr[i]>=target)
            d++;
        }
        ar[0]=c;
        ar[1]=d;
        return ar;
    }
}
