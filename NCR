package javapac;
import java.util.*;
public class NCR {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int r=sc.nextInt();
		int res=factorial(n)/(factorial(r)*factorial(n-r));
		System.out.println(res);

	}
	public static int factorial(int n) {
		int sum=1;
		for(int i=1;i<=n;i++) {
			 sum=sum*i;
		}
		return sum;
	}

}
