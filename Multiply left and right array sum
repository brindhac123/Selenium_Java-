class Solution {
    public int multiply(int[] arr) {
        // code here
        int n=arr.length/2;
        int lsum=0,rsum=0;
        for(int i=0;i<n;i++){
            lsum+=arr[i];
        }
        for(int i=n;i<arr.length;i++){
            rsum+=arr[i];
        }
        return lsum*rsum;
    }
}
