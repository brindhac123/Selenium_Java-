class Solution {
    static ArrayList<Integer> leaders(int arr[]) {
        // code here
        int c=0;
        ArrayList<Integer> val=new ArrayList<>();
        for(int i=0;i<arr.length;i++){
            c=0;
            for(int j=i+1;j<arr.length;j++){
                if(arr[i]>=arr[j])
                c+=1;
            }
            if(c==arr.length-i-1)
            val.add(arr[i]);
        }
        
        return val;
    }
}
